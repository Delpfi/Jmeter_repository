          <stringProp name="parameters"></stringProp>
          <stringProp name="script">def old_newsIndex = vars.get(&quot;newsIndex&quot;).toInteger();
def Index = 1;
def number_page1 =(int)((old_newsIndex/10)+1.0);  //номер пакета с новостями
def number_Index = (int)((old_newsIndex - Index)%10); // номер новости

vars.put(&apos;number_page&apos;, number_page1.toString());
vars.put(&apos;number_Index&apos;, number_Index.toString()); 
log.info(vars.get(&quot;number_Index&quot;));</stringProp>
