            <stringProp name="parameters"></stringProp>
            <stringProp name="script">//Распарсить html 
import groovy.json.JsonSlurper;
import org.jsoup.Jsoup;
import org.jsoup.nodes.Document;

def jsonSlurper = new JsonSlurper();
def response = jsonSlurper.parseText(prev.getResponseDataAsString());//response.content - ответ от сайта, html
vars.put(&quot;text_news_1&quot;, response.content.toString()); //response.content - выборка ключа. text_news_1 - значение, положить сюда, наши новости-тело.
vars.put(&quot;head_text&quot;, response.caption.toString()) // caption - ключ в html, с заголовком
String head_text = vars.get(&quot;head_text&quot;);// сам заголовок
String html = vars.get(&quot;text_news_1&quot;); //записал в строковую переменную, новость
Document doc = Jsoup.parse(html); // привел к типу док
String newsText = ${newsIndex}+&quot;-&quot;+head_text +&quot;.&quot;+ doc.text();
log.info(newsText);


props.put(&quot;newsText&quot;, newsText);</stringProp>
